{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0%, not dead"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-private-methods",
    "@babel/plugin-transform-async-to-generator",
    ["@babel/plugin-transform-runtime", {
      "corejs": 3
    }],
    "dynamic-import-webpack"
  ]
}